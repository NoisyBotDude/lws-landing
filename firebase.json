{
	"hosting": [
		{
			"target": "default",
			"public": "build",
			"ignore": [
				"firebase.json",
				"**/.*",
				"**/node_modules/**"
			],
			"rewrites": [
				{
					"source": "**",
					"destination": "/index.html"
				}
			]
		},
		{
			"target": "sanity-studio",
			"public": "sanity-studio/dist",
			"ignore": [
				"firebase.json",
				"**/.*",
				"**/node_modules/**"
			]
		}
	]
}